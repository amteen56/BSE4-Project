<body>

    @if (ViewBag.Message != "")
    {

        if (ViewBag.Message == "Invalid Error Occured Try Again")
        {<div class="error">@ViewBag.Message</div>}
        else
        { <div class="info">@ViewBag.Message</div>}
    }
@Html.BeginForm("Report", "Game", FormMethod.Post)
{
    @Html.AntiForgeryToken()

    <div role="main" class="form-all">
        <ul class="form-section page-section">
            <li id="cid_1" class="form-input-wide" data-type="control_head">
                <div class="form-header-group ">
                    <div class="header-text httal htvam">
                        <h1 id="header_1" class="form-header" data-component="header">
                            Feedback Form
                        </h1>
                        <div id="subHeader_1" class="form-subHeader">
                            We would love to hear your thoughts, concerns or problems with anything so we can improve!
                        </div>
                    </div>
                </div>
            </li>
            <li class="form-line" data-type="control_radio" id="id_3">
                <label class="form-label form-label-top form-label-auto" id="label_3" for="input_3"> Feedback Type </label>
                <div id="cid_3" class="form-input-wide">
                    <div class="form-multiple-column" data-columncount="3" data-component="radio">

                        <span class="form-radio-item">
                            <span class="dragger-item">
                            </span>
                            <input type="radio" class="form-radio" onclick='javascript:hider(1)' name="breport" value="Bug Reports" />
                            <label id="label_input_3_1" for="input_3_1"> Bug Reports </label>
                        </span>
                        <span class="form-radio-item">
                            <span class="dragger-item">
                            </span>
                            <input type="radio" class="form-radio" onclick='hider(0)' name="breport" checked value="Report User" />
                            <label id="label_input_3_2" for="input_3_2"> Report User </label>
                        </span>
                    </div>
                </div>
            </li>
            <li class="form-line jf-required" data-type="control_textarea" id="id_4">
                <label class="form-label form-label-top form-label-auto" id="label_4" for="input_4">
                    Describe Feedback:
                    <span class="form-required">
                        *
                    </span>
                </label>
                <div id="cid_4" class="form-input-wide jf-required">
                    <textarea id="input_4" class="form-textarea validate[required]" name="q4_describeFeedback" cols="40" rows="6" data-component="textarea" required=""></textarea>
                </div>
            </li>
            <li class="form-line jf-required" data-type="UserName" id="id_8">
                <label class="form-label form-label-top form-label-auto" id="label_8" for="first_8">
                    UserName
                    <span class="form-required">
                        *
                    </span>
                </label>
                <div id="cid_8" class="form-input-wide jf-required">
                    <div data-wrapper-react="true">
                        <span class="form-sub-label-container" style="vertical-align:top">
                            <input type="text" id="first_8" name="Uname" class="form-textbox validate[required]" size="32" value="" data-component="text" aria-labelledby="label_8 sublabel_first" required="" />
                        </span>

                    </div>
                </div>
            </li>
            <li class="form-line jf-required" id="auname">
                <label class="form-label form-label-top form-label-auto" id="label_6" for="input_6" required="">
                    Against UserName
                    <span class="form-required">
                        *
                    </span>
                </label>
                <div id="cid_6" class="form-input-wide jf-required">
                    <input type="text" id="input_6" name="auname" class="form-textbox" size="32" value="" data-component="text" />
                </div>
            </li>
            <li class="form-line" data-type="control_button" id="id_2">
                <div id="cid_2" class="form-input-wide">
                    <div style="margin-left:156px" class="form-buttons-wrapper">
                        <button id="input_2" type="submit" class="form-submit-button form-submit-button-steel" data-component="button">
                            Submit Feedback
                        </button>
                    </div>
                </div>
            </li>
            <li style="display:none">
                Should be Empty:
                <input type="text" name="website" value="" />
            </li>
        </ul>
    </div>

}
</body>
</html>
