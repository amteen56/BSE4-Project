    <script>
        function hider(x) {
            if (x == 0) {
                document.getElementById('auname').style.display = 'block';
                document.getElementById("auname").required = true;
            }
            else {
                document.getElementById('auname').style.display = 'none';
                document.getElementById("auname").required = false;
